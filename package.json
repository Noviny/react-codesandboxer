{
  "name": "codesandboxer-repo",
  "version": "1.0.0",
  "description": "A simple react component to help easily deploy an example to codesandbox",
  "scripts": {
    "test": "yarn jest",
    "build": "bolt && bolt ws exec -- babel src -d dist --ignore  **/*.test.js",
    "lint": "yarn eslint .",
    "prepublishOnly": "yarn test && yarn flow && yarn build",
    "dev:csb": "bolt w codesandboxer exec -- babel src -d dist --ignore  **/*.test.js --watch",
    "version:csb": "bolt w codesandboxer exec -- npm version",
    "dev:rcsb": "bolt w react-codesandboxer exec -- babel src -d dist --ignore  **/*.test.js --watch",
    "version:rcsb": "bolt w react-codesandboxer exec -- npm version"
  },
  "keywords": [
    "codesandbox",
    "react",
    "react-component"
  ],
  "author": "Ben Conolly",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.64.0",
    "jest": "^22.1.4",
    "jest-in-case": "^1.0.2",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0",
    "react-dom": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "bolt": "^0.20.5",
    "codesandbox": "^1.1.14",
    "form-data": "^2.3.2",
    "isomorphic-unfetch": "^2.0.0",
    "meow": "^5.0.0",
    "pkg-dir": "^2.0.0",
    "pkg-up": "^2.0.0",
    "react-node-resolver": "^1.0.1"
  },
  "repository": "https://github.com/Noviny/codesandboxer.git",
  "bolt": {
    "workspaces": [
      "packages/*"
    ]
  }
}
