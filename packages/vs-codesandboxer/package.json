{
    "name": "vs-codesandboxer",
    "displayName": "vs-codesandboxer",
    "description": "upload to codesandbox from a single entry file",
    "version": "0.0.1",
    "publisher": "noviny",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "cli",
        "codesandbox"
    ],
    "activationEvents": [
        "onCommand:extension.codesandboxer"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.codesandboxer",
                "title": "Deploy to codesandbox"
            }
        ],
        "menus": {
            "editor/context": [{
                "when": "resourceLangId == javascript",
                "command": "extension.codesandboxer"
          }]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
      "codesandboxer-fs": "^0.0.0",
      "pkg-up": "^2.0.0"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
