{
    "name": "vs-codesandboxer",
    "displayName": "vs-codesandboxer",
    "description": "upload to codesandbox from a single entry file",
    "version": "0.1.5",
    "publisher": "noviny",
    "engines": {
        "vscode": "^1.23.0"
    },
    "activationEvents": [
        "onCommand:extension.vs-codesandboxer"
    ],
    "main": "./extension",
    "repository": "https://github.com/Noviny/codesandboxer.git",
    "contributes": {
        "commands": [
            {
                "command": "extension.vs-codesandboxer",
                "title": "Deploy to codesandbox"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "resourceLangId == javascript",
                    "command": "extension.vs-codesandboxer"
                }
            ]
        }
    },
    "scripts": {
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "codesandboxer-fs": "^0.1.1",
        "pkg-up": "^2.0.0",
        "pkg-dir": "^2.0.0"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
